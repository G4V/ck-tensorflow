{
  "run_deps": {
    "python": {
      "local": "yes",
      "name": "Python interpreter",
      "sort": 5,
      "tags": "compiler,python"
    },
    "tensorflow-api-model":{
      "local": "yes",
      "name": "TensorFlow Object recognition model",
      "sort": 10,
      "tags": "model,tensorflowapimodel"
    },
    "lib-tensorflow": {
      "local": "yes",
      "name": "Tensorflow library",
      "sort": 40,
      "tags": "lib,tensorflow"
    }
  },
  "data_name": "TensorFlow API Models",
  "program": "yes",
  "process_in_tmp": "yes",
  "no_compile": "yes",
  "no_target_file": "yes",
  "run_cmds": {
    "train":{
      "ignore_return_code": "no",
      "name":"Training Job",
      "sort":5,
      "run_time":{
        "run_cmd_main": "$<<CK_ENV_COMPILER_PYTHON_FILE>>$ $<<CK_ENV_TENSORFLOW_MODELS_ROOT>>$/train.py --logtostderr --pipeline_config_path=$<<CK_ENV_MODEL_TENSORFLOW_API_MODEL>>$/pipeline.config --train_dir=$<<CK_ENV_MODEL_TENSORFLOW_API_MODEL>>$/train"
      }
    },
    "evaluation":{
      "ignore_return_code": "no",
      "name":"Evaluation Job",
      "sort":10,
      "run_time":{
        "run_cmd_main": "$<<CK_ENV_COMPILER_PYTHON_FILE>>$ $<<CK_ENV_TENSORFLOW_MODELS_ROOT>>$/eval.py --logtostderr --pipeline_config_path=$<<CK_ENV_MODEL_TENSORFLOW_API_MODEL>>$/pipeline.config --checkpoint_dir=$<<CK_ENV_MODEL_TENSORFLOW_API_MODEL>>$/train --eval_dir=$<<CK_ENV_MODEL_TENSORFLOW_API_MODEL>>$/eval"
      } 
    }
  },
  "tags": [
    "tensorflow",
    "tensorflow-api",
    "object-detection"
  ]
}
